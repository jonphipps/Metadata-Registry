        "symfony/dom-crawler"            : "^3.3",
        "symfony/filesystem"             : "^3.3",
        "symfony/intl"                   : "^5.2",
        "symfony/psr-http-message-bridge": "^1.0",
        "techlab/smartwizard"            : "^4.2",
        "thedevsaddam/laravel-schema"    : "^2.0",
        "venturecraft/revisionable"      : "^1.28",
        "vlucas/phpdotenv"               : "~2.2",
        "willdurand/negotiation"         : "^2.0",
        "yajra/laravel-datatables-oracle": "^8.0", "ext-json": "*"
    },
    "require-dev":
    {
        "backpack/generators": "^1.1",
        "barryvdh/laravel-debugbar": "^3.0",
        "barryvdh/laravel-ide-helper": "dev-Make-Model-Helper-Optional as 2.4.2",
        "filp/whoops": "~2.0",
        "flow/jsonpath": "^0.3.4",
        "fzaninotto/faker": "^1.6",
        "infyomlabs/laravel-generator": "5.5.x-dev",
        "jtant/laravel-env-sync": "^1.3",
        "kint-php/kint": "^2.0",
        "laravel/browser-kit-testing": "^2.0",
        "laravel/dusk": "^2.0",
        "mockery/mockery": "0.9.*",
        "mpociot/laravel-test-factory-helper": "^0.5.0",
        "nunomaduro/collision": "^1.1",
        "orangehill/iseed": "^2.3",
        "pbmedia/laravel-browser-kit-macro": "^1.0",
        "phpunit/phpunit": "~6.0",
        "recca0120/laravel-tracy": "^1.8",
        "spatie/laravel-db-snapshots": "^1.0",
        "spatie/phpunit-snapshot-assertions": "^1.0",
        "stechstudio/laravel-vfs-adapter": "^2.0",
        "tightenco/mailthief": "^0.3.4",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "ImportVocab\\": "plugins/jpAdminGeneratorPlugin/lib/ImportVocab/",
            "model\\"      : "lib/model/",
            "apps\\"       : "apps/",
            "App\\"        : "app/",
            "Database\\"   : "database/"
        },
        "classmap": [
            "lib/model",
            "database"
        ],
        "files": [
            "app/helpers.php",
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "extra":
    {
        "laravel":
        {
            "dont-discover": [
                "backpack/generators",
                "laravel/dusk"
            ]
        }
    },
    "scripts":
    {
        "post-root-package-install" : [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate"
        ]
    },
    "config":
    {
        "process-timeout": 900,
        "cache-dir:": "../",
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "include-path": [
        "lib/model"
    ]
}
