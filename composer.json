    "minimum-stability": "dev",
    "prefer-stable": true,
    "require":
    {
        "php": ">=7.1.0",
        "ext-intl": "*",
        "arcanedev/log-viewer": "~4.0",
        "arcanedev/no-captcha": "^5.0",
        "asimlqt/php-google-spreadsheet-client": "3.0.*",
        "ausi/slug-generator": "^1.0",
