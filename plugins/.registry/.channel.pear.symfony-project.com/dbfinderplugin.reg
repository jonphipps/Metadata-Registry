a:22:{s:7:"attribs";a:6:{s:5:"xmlns";s:35:"http://pear.php.net/dtd/package-2.0";s:11:"xmlns:tasks";s:33:"http://pear.php.net/dtd/tasks-1.0";s:9:"xmlns:xsi";s:41:"http://www.w3.org/2001/XMLSchema-instance";s:15:"packagerversion";s:5:"1.6.2";s:7:"version";s:3:"2.0";s:18:"xsi:schemaLocation";s:147:"http://pear.php.net/dtd/tasks-1.0 http://pear.php.net/dtd/tasks-1.0.xsd http://pear.php.net/dtd/package-2.0 http://pear.php.net/dtd/package-2.0.xsd";}s:4:"name";s:14:"DbFinderPlugin";s:7:"channel";s:24:"pear.symfony-project.com";s:7:"summary";s:137:"The `DbFinder` is a symfony plugin that provides an easy API for finding Model objects, whether the underlying ORM is Propel or Doctrine.";s:11:"description";s:379:"`DbFinder` can be seen as:

  * A usability layer to ease the use of Propel's Criteria object and Peer classes
  * An extension to Propel's limited capabilities to provide:
    - complex joins
    - custom hydration of related objects and columns
    - schema and relation introspection
  * A compatibility layer to allow plugins to work with Propel 1.2, Propel 1.3, and Doctrine";s:4:"lead";a:4:{s:4:"name";s:18:"Francois Zaninotto";s:4:"user";s:10:"fzaninotto";s:5:"email";s:38:"francois.zaninotto@symfony-project.com";s:6:"active";s:3:"yes";}s:4:"date";s:10:"2009-04-17";s:4:"time";s:8:"16:16:34";s:7:"version";a:2:{s:7:"release";s:5:"1.2.1";s:3:"api";s:5:"1.2.1";}s:9:"stability";a:2:{s:7:"release";s:6:"stable";s:3:"api";s:6:"stable";}s:7:"license";a:2:{s:7:"attribs";a:1:{s:3:"uri";s:38:"http://www.symfony-project.com/license";}s:8:"_content";s:11:"MIT license";}s:5:"notes";s:1:"-";s:8:"contents";a:1:{s:3:"dir";a:2:{s:7:"attribs";a:1:{s:4:"name";s:1:"/";}s:4:"file";a:49:{i:0;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"5c15859726259f3529835b6877d895cf";s:4:"name";s:71:"data/generator/DbFinderAdmin/default/skeleton/actions/actions.class.php";s:4:"role";s:4:"data";}}i:1;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"b529ad3a89387d6094c2ef3f565a8356";s:4:"name";s:66:"data/generator/DbFinderAdmin/default/skeleton/config/generator.yml";s:4:"role";s:4:"data";}}i:2;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"7d8ddead9c85d54aac88ad8cfcdc7368";s:4:"name";s:71:"data/generator/DbFinderAdmin/default/template/actions/actions.class.php";s:4:"role";s:4:"data";}}i:3;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"599788fbcbf42755f1b9297cd7b81bdb";s:4:"name";s:73:"data/generator/DbFinderAdmin/default/template/templates/_edit_actions.php";s:4:"role";s:4:"data";}}i:4;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"d41d8cd98f00b204e9800998ecf8427e";s:4:"name";s:72:"data/generator/DbFinderAdmin/default/template/templates/_edit_footer.php";s:4:"role";s:4:"data";}}i:5;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"7076dd193706f99303dcb91494645764";s:4:"name";s:70:"data/generator/DbFinderAdmin/default/template/templates/_edit_form.php";s:4:"role";s:4:"data";}}i:6;a:1:{s:7:"attribs";a:3:{s:6:"md5sum";s:32:"d41d8cd98f00b204e9800998ecf8427e";s:4:"name";s:72:"data/generator/DbFinderAdmin/default/temp